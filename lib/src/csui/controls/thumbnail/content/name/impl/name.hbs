{{#if displayIcon}}
  {{#if defaultAction}}
    {{#if inactive}}
      <div class="csui-thumbnail-overview-icon csui-thumbnail-no-default-action"></div>
    {{else}}
      <a href="{{defaultActionUrl}}" class="csui-thumbnail-overview-icon csui-thumbnail-default-action" tabindex="0"/>
    {{/if}}
  {{else}}
    <div class="csui-thumbnail-overview-icon csui-thumbnail-no-default-action"></div>
  {{/if}}
{{/if}}
{{#if defaultAction}}
  {{#if inactive}}
    <div class="csui-thumbnail-name csui-thumbnail-name-{{cid}}">
      <span class="csui-thumbnail-name-justify-div">
        <span class="csui-thumbnail-name-value csui-thumbnail-no-default-action"
           title="{{name}}" aria-label="{{nameNoOpenAria}}">{{name}}</span>
      </span>
    </div>
  {{else}}
    <div class="csui-thumbnail-name csui-thumbnail-name-{{cid}}">
      <a href="{{defaultActionUrl}}"
         class="csui-thumbnail-name-value csui-thumbnail-default-action csui-thumbnail-name-justify-div"
         title="{{name}}" aria-label="{{nameAria}}">
        <span class="csui-thumbnail-name-link-text">{{name}}</span>
      </a>
    </div>
  {{/if}}
{{else}}
  <div class="csui-thumbnail-name csui-thumbnail-name-{{cid}}">
    <span class="csui-thumbnail-name-justify-div">
      <span class="csui-thumbnail-name-value csui-thumbnail-no-default-action"
         title="{{name}}" aria-label="{{nameNoOpenAria}}">{{name}}</span>
    </span>
  </div>
{{/if}}

