
<div class="binf-panel-heading outlook-heading" >{{sectionTitle}} 
    
</div>
<div class="wkspSearch">
    <div class="wkspSearchInputName">
        <table class="wkspNameBox">
            <tr>
                <td class="wkspInputWithClear">
                    <input class="wkspInput wkspSearchInput wkspSearchName" id="wkspName" type="search" placeholder="{{wkspNamePlaceholder}}" />
                    <span class="searchClear wkspIcon" id="searchClearButton" title="{{searchClearTooltip}}"></span>
                </td>
                <td class="wkspSearchButton">
                    <a id="wkspTypeButton" href="javascript:void(0);" tabindex="-1"><div class="wkspIcon searchTypeIcon" title="{{selectWkspType}}" tabindex="0"/></a>
                </td>
                <td class="wkspSearchButton">
                    <a id="searchButton" href="javascript:void(0);" tabindex="-1"><div class="wkspIcon searchIcon" title="{{searchTooltip}}" tabindex="0"/></a>
                </td>
            </tr>
            <tr>
                <td colspan="3">
                    <div id="typeDropdown" class="wkspTypeDropdown hiddenArea"></div>
                    <div id="wkspTypeMsg" class="wkspTypeMsg hiddenArea"></div>
                </td>
            </tr>
        </table>
    </div>
</div>

<div class="binf-panel-heading outlook-heading hiddenArea" id="searchResultHeading">
    <span class="headerIcon backwardIcon" tabindex="0" id="searchResultBackButton"></span>
    <span >{{resultTitle}}</span>
</div> 
<div id="search-result"></div>

<div class="wkspMessageArea" id="searchMsgArea"></div>




